<%- include("partials/header") -%>
        
        <h2 class="heading">Home</h2>
        <p><%= homeStartingContent %></p>
        <hr size="1" noshade>

        <% posts.forEach(function(post) { %>
                <h2 class="heading"><%= post.title %></h2>
                <p><%= post.content.substring(0,101) %>... <a href="/posts/<%= post.title %>">Read More</a></p>
                <hr size="1" noshade>
        <% }); %>
        
<%- include("partials/footer") -%>